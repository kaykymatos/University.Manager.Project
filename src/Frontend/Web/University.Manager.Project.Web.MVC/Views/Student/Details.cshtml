@model University.Manager.Project.Web.MVC.Models.StudentViewModel

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>


<div>
    <h4>Student</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Id
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Id)
        </dd>
        <dt class="col-sm-2">
            Name
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
        <dt class="col-sm-2">
            Email
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Email)
        </dd>
        <dt class="col-sm-2">
            Register Code
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.RegisterCode)
        </dd>
        <dt class="col-sm-2">
            Course
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CourseId)
        </dd>
        
        <dt class="col-sm-2">
            Creation Data
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CreationData)
        </dd>
        <dt class="col-sm-2">
            Updated Data
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.UpdatedData)
        </dd>
    </dl>
</div>

<div>
    <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-warning">Edit</a>
    <a asp-action="Index" class="btn btn-secondary">Back to List</a>
</div>
